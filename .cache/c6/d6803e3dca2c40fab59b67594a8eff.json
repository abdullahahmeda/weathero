 return weatherInfoResponse;\r\n  }\r\n\r\n  useEffect(() => {\r\n    setIsLoaded(false);\r\n    setError(null);\r\n    getLatAndLon(city)\r\n      .then(res =>\r\n        getWeatherInfo(res[0], res[1])\r\n          .then(weatherInfo => {\r\n            setWeatherInfo(weatherInfo);\r\n            setIsLoaded(true);\r\n          })\r\n          .catch(e => {\r\n            setError(\r\n              \"Couldn't fetch weather data. Please check your internet connection\"\r\n            );\r\n            setIsLoaded(true);\r\n          })\r\n      )\r\n      .catch(e => {\r\n        setError(\"Couldn't fetch this city\");\r\n        setIsLoaded(true);\r\n      });\r\n  }, [city]);\r\n\r\n  if (isLoaded) {\r\n    if (error) {\r\n      return <div>{error}</div>;\r\n    }\r\n    return (\r\n      <div className=\"weather-card z-depth-1 p-relative\">\r\n        <p>{city}</p>\r\n        <p className=\"weather-temp p-absolute\">{weatherInfo.temp}&deg; C</p>\r\n      </div>\r\n    );\r\n  }\r\n  return <div>Loading...</div>;\r\n};\r\n\r\nexport default Card;\r\n"},"lineCount":null}},"error":null,"hash":"e66eb30f3c1867d884c536a9613f45b2","cacheData":{"env":{}}}